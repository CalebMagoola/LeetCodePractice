import java.util.*;
class Solution {
    public int percentageLetter(String s, char letter) {
        int count = 0;
        double total = 0;
        for(int i = 0; i < s.length(); i++){
            if(s.charAt(i) == letter)
                count++;
            total++;
                
        }
        double ans = count/total;
        return (int)Math.floor(ans * 100);
    }
}
