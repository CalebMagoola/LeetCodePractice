class Solution {
    public int distributeCandies(int[] candyType) {
        Set<Integer> set = new HashSet();
        int types = 0;
        for(int i = 0; i < candyType.length; i++){
            if(!set.contains(candyType[i])){
                set.add(candyType[i]);
                types++;
            }
        }
        if(types <= candyType.length/2)
            return types;
        else
            return candyType.length/2;
    }
}
