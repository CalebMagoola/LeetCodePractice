class Solution {
    public int[] runningSum(int[] nums) {
        int count = 0;
        int[] ans = new int[nums.length];
        for(int i = 0; i < nums.length; i++){
            for(int j = 0;j <=i;j++){
                count +=nums[j];
            }
            ans[i] = count;
            count = 0;
        }
        return ans;
    }
}
